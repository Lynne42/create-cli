{
    "name": "create-cli",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "create-react": "node tasks/create-react.js",
        "create-next": "node tasks/create-next.js",
        "create-umi": "node tasks/create-umi.js",
        "build": "lerna exec -- babel src -d dist --config-file ../../babel.config.js",
        "publish": "npm run build && lerna publish --force-publish",
        "prepare": "husky install",
        "commit": "npx git-cz"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "commander": "^9.3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.18.5",
        "@babel/node": "^7.18.5",
        "@babel/preset-env": "^7.18.2",
        "@commitlint/cli": "^13.2.1",
        "@commitlint/config-conventional": "^13.2.0",
        "babel-preset-minify": "^0.5.1",
        "commitizen": "^1.0.4",
        "cz-conventional-changelog": "^3.0.1",
        "eslint": "^8.0.1",
        "husky": "^7.0.2",
        "lerna": "^4.0.0",
        "lint-staged": "^13.0.2",
        "prettier": "^2.4.1"
    },
    "lint-staged": {
        "*.{js,json,yml,yaml,css,scss,ts,tsx,md}": [
            "prettier --write --ignore-unknown"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
