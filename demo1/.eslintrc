{
    "plugins": ["react", "prettier"],
    "extends": ["react-app", "shared-config", "prettier"],
    "rules": {
        "no-console": "off",
        "no-plusplus": ["error", {"allowForLoopAfterthoughts": true}],
        "react/jsx-filename-extension": [
            "warn",
            {
                "extensions": [".ts", ".tsx"]
            }
        ]
    }
}
